{
  "name" : "ya-prometheus-connector",
  "config" : {
    "topics":"KAFKA_TOPIC",
    "connector.class" : "io.confluent.connect.prometheus.PrometheusMetricsSinkConnector",
    "tasks.max" : "1",
    "bootstrap.servers": "BOOTSTRAP:9092",
    "ssl.endpoint.identification.algorithm": "https",
    "security.protocol": "SASL_SSL",
    "sasl.mechanism": "PLAIN",
    "sasl.jaas.config": "PUT CREDS IN HERE",
    "request.timeout.ms": "20000",
    "retry.backoff.ms": "500",
    "reporter.bootstrap.servers":"BOOTSTRAP:9092",
    "reporter.error.topic.name": "",
    "reporter.result.topic.name": "",
    "prometheus.listener.url": "http://XXXXXXXXX",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.basic.auth.credentials.source": "USER_INFO",
    "value.converter.schema.registry.basic.auth.user.info": "REST_PROXY_CREDS",
    "value.converter.schema.registry.url": "REST_PROXY_URL",
    "reporter.result.topic.replication.factor": "3",
    "reporter.error.topic.replication.factor": "3",
    "behavior.on.error": "log",
    "confluent.topic.bootstrap.servers": "BOOTSTRAP:9092",
    "confluent.topic.replication.factor": "3",
    "confluent.license": "",
    "confluent.topic.security.protocol": "SASL_SSL",
    "confluent.topic.sasl.jaas.config": "PUT CREDS IN HERE",
    "confluent.topic.ssl.endpoint.identification.algorithm": "https",
    "confluent.topic.sasl.mechanism": "PLAIN",
    "confluent.topic": "_confluent-command",
    "confluent.topic.retry.backoff.ms": "500",
    "confluent.topic.request.timeout.ms": "60000",
    "producer.bootstrap.servers": "BOOSTRAP:9092",
    "producer.ssl.endpoint.identification.algorithm": "https",
    "producer.security.protocol": "SASL_SSL",
    "producer.sasl.mechanism": "PLAIN",
    "producer.sasl.jaas.config": "PUT CREDS HERE",
    "producer.request.timeout.ms": "20000",
    "producer.retry.backoff.ms": "500",
    "consumer.bootstrap.servers": "BOOTSTRAP:9092",
    "consumer.ssl.endpoint.identification.algorithm": "https",
    "consumer.security.protocol": "SASL_SSL",
    "consumer.sasl.mechanism": "PLAIN",
    "consumer.sasl.jaas.config": "PUT CREDS HERE",
    "consumer.request.timeout.ms": "20000",
    "consumer.retry.backoff.ms": "500"
  }
}